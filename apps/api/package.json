{
  "name": "@myhealthurl/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@fastify/cookie": "^10.0.1",
    "@fastify/cors": "^10.0.1",
    "@myhealthurl/shared": "workspace:*",
    "better-sqlite3": "^11.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.37.0",
    "fastify": "^5.1.0",
    "jose": "4",
    "pako": "^2.1.0",
    "ua-parser-js": "^1.0.39",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.1",
    "@types/pako": "^2.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "drizzle-kit": "^0.30.1",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vitest": "^2.1.6"
  }
}
